{
  "name": "@macil/rcon-ts",
  "version": "1.2.3",
  "description": "A modern RCON client implementation written in TypeScript (targeting ES2015) and is async/await friendly.",
  "keywords": [
    "rcon",
    "async-await",
    "typescript",
    "minecraft",
    "factorio"
  ],
  "main": "dist/src/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "!*.test.*"
  ],
  "scripts": {
    "prepare": "husky install",
    "prepack": "tsc",
    "test": "tsc --noEmit && jest"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "repository": "github:Macil/rcon-ts",
  "contributors": [
    "Levin Ricket <me@levrik.io>",
    "electricessence",
    "Chris Cowan <agentme49@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "babel-jest": "^27.5.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "2.5.1",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@types/node": "*",
    "tslib": "^2.3.1"
  }
